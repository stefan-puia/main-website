{
  "name": "main-website",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "minify-js:main": "minify src/assets/js/main.js > src/assets/js/main.min.js",
    "minify-js:util": "minify src/assets/js/util.js > src/assets/js/util.min.js",
    "minify-js": "run-p minify-js:main minify-js:util",
    "minify-css": "minify src/assets/css/main.css > src/assets/css/main.min.css",
    "sass-compile": "sass src/assets/sass/main.scss src/assets/css/main.css",
    "css-compile": "run-s sass-compile minify-css",
    "clean": "rimraf public",
    "dev": "run-s clean dev:start",
    "dev:start": "cross-env NODE_ENV=development eleventy --serve",
    "clean-compile-minify": "run-s clean css-compile minify-js",
    "prestart": "npm run clean-compile-minify",
    "start": "concurrently --kill-others \"npm run dev\" \"npm run netlify-cms-proxy\"",
    "prebuild": "npm run clean-compile-minify",
    "build": "cross-env NODE_ENV=production eleventy",
    "postbuild": "purgecss -c purgecss.config.js",
    "netlify-cms-proxy": "netlify-cms-proxy-server"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "2.0.1",
    "concurrently": "8.2.2",
    "cross-env": "7.0.3",
    "follow-redirects": "1.15.9",
    "luxon": "3.5.0",
    "markdown-it": "14.1.0",
    "minify": "10.5.2",
    "netlify-cms-proxy-server": "1.3.24",
    "npm-run-all2": "6.2.4",
    "nunjucks": "3.2.4",
    "purgecss": "5.0.0",
    "rimraf": "5.0.10",
    "sass": "1.80.2"
  }
}
